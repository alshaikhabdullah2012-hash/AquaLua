# AquaLua Complete Syntax Test - Every Feature
print("=== COMPLETE AQUALUA SYNTAX TEST ===")

# 1. VARIABLE DECLARATIONS
let x = 42
let y = 3.14
let name = "AquaLua"
let flag = true
let empty = null
const PI = 3.14159

# 2. DATA TYPES
let integer = 100
let float_num = 99.99
let string1 = "double quotes"
let string2 = 'single quotes'
let boolean_true = true
let boolean_false = false
let array = [1, 2, 3, "mixed", true]
let dict = {"key": "value", "number": 42}

# 3. OPERATORS - Arithmetic
let add_result = 10 + 5
let sub_result = 10 - 5
let mul_result = 10 * 5
let div_result = 10 / 5
let mod_result = 10 % 3

# 4. OPERATORS - Comparison
let eq_test = 5 == 5
let ne_test = 5 != 3
let lt_test = 3 < 5
let gt_test = 5 > 3
let le_test = 3 <= 5
let ge_test = 5 >= 3

# 5. OPERATORS - Logical
let and_test = true and false
let or_test = true or false

# 6. CONTROL FLOW - If statements (brace syntax)
if x > 30 {
    print("x is greater than 30")
} elif x > 20 {
    print("x is greater than 20")
} else {
    print("x is 20 or less")
}

# 7. CONTROL FLOW - If statements (colon syntax)
if y > 3:
    print("y is greater than 3")
elif y > 2:
    print("y is greater than 2")
else:
    print("y is 2 or less")

# 8. LOOPS - While (brace syntax)
let counter = 0
while counter < 3 {
    print("Counter:", counter)
    counter = counter + 1
}

# 9. LOOPS - While (colon syntax)
let counter2 = 0
while counter2 < 2:
    print("Counter2:", counter2)
    counter2 = counter2 + 1

# 10. LOOPS - For (brace syntax)
for i in range(3) {
    print("For loop i:", i)
}

# 11. LOOPS - For (colon syntax)
for j in [1, 2, 3]:
    print("For loop j:", j)

# 12. FUNCTIONS - Basic (brace syntax)
fn add_numbers(a, b) {
    return a + b
}

# 13. FUNCTIONS - Basic (colon syntax)
fn multiply_numbers(a, b):
    return a * b

# 14. FUNCTIONS - No parameters
fn say_hello() {
    return "Hello World!"
}

# 15. FUNCTIONS - Multiple statements
fn complex_function(x, y) {
    let temp = x + y
    let result = temp * 2
    return result
}

# 16. FUNCTION CALLS
print("add_numbers(5, 3):", add_numbers(5, 3))
print("multiply_numbers(4, 6):", multiply_numbers(4, 6))
print("say_hello():", say_hello())
print("complex_function(2, 3):", complex_function(2, 3))

# 17. BUILT-IN FUNCTIONS
print("len([1,2,3]):", len([1, 2, 3]))
print("str(42):", str(42))
print("int('123'):", int('123'))
print("float('3.14'):", float('3.14'))
print("abs(-5):", abs(-5))
print("min(3, 7):", min(3, 7))
print("max(3, 7):", max(3, 7))
print("pow(2, 3):", pow(2, 3))

# 18. ERROR HANDLING - Try/Except (brace syntax)
try {
    let bad_division = 10 / 0
} except {
    print("Caught an error in brace syntax")
}

# 19. ERROR HANDLING - Try/Except (colon syntax)
try:
    let another_error = 5 / 0
except:
    print("Caught an error in colon syntax")

# 20. ERROR HANDLING - Try/Except/Finally
try {
    let risky_operation = 1 / 1
    print("No error occurred")
} except {
    print("Error occurred")
} finally {
    print("Finally block executed")
}

# 21. CLASSES - Basic (brace syntax)
class Person {
    fn __init__(name, age) {
        self.name = name
        self.age = age
    }
    
    fn get_name() {
        return self.name
    }
    
    fn get_age() {
        return self.age
    }
}

# 22. CLASSES - Basic (colon syntax)
class Animal:
    fn __init__(species):
        self.species = species
    
    fn speak():
        return "The " + self.species + " makes a sound"

# 23. ARRAY/LIST OPERATIONS
let test_array = [1, 2, 3, 4, 5]
print("Array:", test_array)
print("Array length:", len(test_array))

