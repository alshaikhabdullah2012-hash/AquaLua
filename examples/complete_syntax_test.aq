# 32. RETURN STATEMENTS
fn early_return(value):
    if value < 0:
        return "Negative"
    return "Positive or Zero"

print("early_return(-5):", early_return(-5))
print("early_return(5):", early_return(5))

# 33. BREAK AND CONTINUE
for i in range(10):
    if i == 3:
        continue
    if i == 7:
        break
    print("Loop value:", i)

# 34. NESTED LOOPS
for outer in range(2):
    for inner in range(2):
        print("Nested:", outer, inner)

# 35. NESTED IF STATEMENTS
if x > 50:
    if y > 100:
        print("Both conditions true")
    else:
        print("Only first condition true")

# 36. FUNCTION WITH CONDITIONALS
fn grade_calculator(score):
    if score >= 90:
        return "A"
    elif score >= 80:
        return "B"
    elif score >= 70:
        return "C"
    else:
        return "F"

print("Grade for 85:", grade_calculator(85))

# 37. RECURSIVE FUNCTION
fn factorial(n):
    if n <= 1:
        return 1
    else:
        return n * factorial(n - 1)

print("Factorial of 5:", factorial(5))

